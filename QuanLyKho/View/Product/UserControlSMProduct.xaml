<UserControl x:Class="QuanLyKho.View.Product.UserControlSMProduct"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuanLyKho.View.Product"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <Button Name="TiKi" Content="Tiki" Style="{StaticResource MenuButtonStyle}" Click="TiKi_Click"/>
            <Button Name="Shopee" Content="Shopee" Style="{StaticResource MenuButtonStyle}" IsEnabled="True"/>
            <Button Name="Lazada" Content="Lazada" Style="{StaticResource MenuButtonStyle}" IsEnabled="False"/>
            <Button Name="Sendo" Content="Sendo" Style="{StaticResource MenuButtonStyle}" IsEnabled="False"/>
        </StackPanel>
    </Grid>
</UserControl>
